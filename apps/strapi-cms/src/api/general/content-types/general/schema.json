{
  "kind": "singleType",
  "collectionName": "generals",
  "info": {
    "singularName": "general",
    "pluralName": "generals",
    "displayName": "Generale",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "metaImage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "favicon": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "appleTouchIcon": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "themeVariant": {
      "type": "enumeration",
      "required": true,
      "default": "SEND",
      "enum": [
        "IO",
        "SEND"
      ]
    },
    "locales": {
      "type": "component",
      "component": "shared.locales",
      "repeatable": false,
      "required": true
    },
    "defaultLocale": {
      "type": "enumeration",
      "required": true,
      "default": "it",
      "enum": [
        "it",
        "en",
        "de",
        "fr",
        "sl"
      ]
    },
    "analytics": {
      "type": "component",
      "component": "shared.analytics",
      "repeatable": false
    },
    "pressReleasesParentSlug": {
      "type": "string",
      "default": "press-releases",
      "regex": "^[a-z]+[a-z\\-]*$"
    },
    "siteName": {
      "type": "component",
      "component": "shared.site-name",
      "repeatable": false
    },
    "oneTrustToken": {
      "type": "text"
    }
  }
}
