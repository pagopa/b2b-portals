{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Pagine",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "private": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "slug": {
      "type": "string",
      "regex": "^[a-z0-9]+(?:-[a-z0-9]+)*$",
      "required": true,
      "unique": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "sections": {
      "type": "dynamiczone",
      "components": [
        "sections.editorial",
        "sections.feature",
        "sections.hero",
        "sections.how-to",
        "sections.banner-link",
        "sections.accordion",
        "sections.cards",
        "sections.stripe-link",
        "sections.one-trust",
        "sections.i-frame",
        "sections.form",
        "sections.editorial-switch",
        "sections.video-image",
        "sections.hero-counter",
        "sections.service-carousel",
        "sections.row-text",
        "sections.highlight-box",
        "sections.stats",
        "sections.text-section",
        "sections.framed-video",
        "sections.page-switch",
        "sections.hero-chips",
        "sections.press-release-list",
        "sections.media-resources"
      ],
      "required": true,
      "min": 1,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    }
  }
}
