{
  "collectionName": "components_menu_menu_links",
  "info": {
    "displayName": "Menu Link",
    "description": ""
  },
  "options": {},
  "attributes": {
    "label": {
      "type": "string",
      "required": true
    },
    "page": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::page.page"
    },
    "sublinks": {
      "type": "component",
      "repeatable": true,
      "component": "menu.menu-sub-link"
    },
    "sectionID": {
      "type": "string",
      "regex": "^[a-z]+[a-z\\-]*$"
    },
    "alignRight": {
      "type": "boolean",
      "default": false,
      "required": true
    }
  }
}
